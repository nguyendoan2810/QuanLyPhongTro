@{
    ViewData["Title"] = "Trang khách thuê";
    Layout = "~/Areas/KhachThue/Views/Shared/_KhachThueLayout.cshtml";
}

<main class="lg:ml-64 pt-16 p-6">
    <!-- Dashboard Page -->
    @await Html.PartialAsync("~/Areas/KhachThue/Views/Dashboard/Index.cshtml")

    <!-- Profile Page -->
    @await Component.InvokeAsync("ThongTinCaNhan")

    <!-- Contracts Page -->
    @await Component.InvokeAsync("HopDong")

    <!-- Bills Page -->
    @await Html.PartialAsync("~/Areas/KhachThue/Views/HoaDon/Index.cshtml")

    <!-- Notifications Page -->
    @await Html.PartialAsync("~/Areas/KhachThue/Views/ThongBaoLienHe/Index.cshtml")
</main>
